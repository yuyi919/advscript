{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["_build", "^build"],
      "outputs": ["dist/**", ".next/**", "lib/bs/**", "src/**/*.gen.ts"]
    },
    "_build": {
      "dependsOn": ["^_build"],
      "outputs": ["dist/**", ".next/**", "lib/bs/**"]
    },
    "clean": {
      "outputs": [],
      "cache": false
    },
    "lint": {
      "outputs": [],
      "cache": false
    },
    "dev": {
      "cache": false
    }
  }
}
